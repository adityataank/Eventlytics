{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "experimental": {
        "serverComponentsExternalPackages": [
            "lucide-react"
        ]
    },
    "functions": {
        "api/**": {
            "maxDuration": 10,
            "memory": 256
        }
    },
    "includeFiles": [
        "public/**",
        "build/**"
    ]
}